@ARTICLE{2021MNRAS.502..478G,
       author = {{Goncharov}, Boris and {Reardon}, D.~J. and {Shannon}, R.~M. and {Zhu}, Xing-Jiang and {Thrane}, Eric and {Bailes}, M. and {Bhat}, N.~D.~R. and {Dai}, S. and {Hobbs}, G. and {Kerr}, M. and {Manchester}, R.~N. and {Os{\l}owski}, S. and {Parthasarathy}, A. and {Russell}, C.~J. and {Spiewak}, R. and {Thyagarajan}, N. and {Wang}, J.~B.},
        title = "{Identifying and mitigating noise sources in precision pulsar timing data sets}",
