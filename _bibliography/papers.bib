        title = "{Timing of young radio pulsars - I. Timing noise, periodic modulation, and proper motion}",
      journal = {MNRAS},
     keywords = {methods: data analysis, stars: neutron, pulsars: general, Astrophysics - High Energy Astrophysical Phenomena, Astrophysics - Solar and Stellar Astrophysics},
         year = 2019,
        month = nov,
       volume = {489},
       number = {3},
        pages = {3810-3826},
     abstract = "{The smooth spin-down of young pulsars is perturbed by two non-
        deterministic phenomenon, glitches, and timing noise. Although
        the timing noise provides insights into nuclear and plasma
        physics at extreme densities, it acts as a barrier to high-
        precision pulsar timing experiments. An improved methodology
        based on the Bayesian inference is developed to simultaneously
        model the stochastic and deterministic parameters for a sample
        of 85 high-\textbackslashdot\{E\} radio pulsars observed for
        {\ensuremath{\sim}}10 yr with the 64-m Parkes radio telescope.
        Timing noise is known to be a red process and we develop a
        parametrization based on the red-noise amplitude (A$_{red}$) and
        spectral index ({\ensuremath{\beta}}). We measure the median
        A$_{red}$ to be -10.4\^\{+1.8\}\_\{-1.7\} yr$^{3/2}$ and
        {\ensuremath{\beta}} to be -5.2\^\{+3.0\}\_\{-3.8\} and show
        that the strength of timing noise scales proportionally to
        {\ensuremath{\nu}} $^{1}$|\textbackslashdot\{{\ensuremath{\nu}}
        \}|\^\{-0.6{\ensuremath{\pm}} 0.1\}, where {\ensuremath{\nu}} is
        the spin frequency of the pulsar and
        \textbackslashdot\{{\ensuremath{\nu}} \} is its spin-down rate.
        Finally, we measure significant braking indices for 19 pulsars
        and proper motions for 2 pulsars, and discuss the presence of
        periodic modulation in the arrival times of 5 pulsars.}",
          doi = {10.1093/MNRAS/stz2383},
archivePrefix = {arXiv},
       eprint = {1908.11709},
 primaryClass = {astro-ph.HE},
       adsurl = {https://ui.adsabs.harvard.edu/abs/2019MNRAS.489.3810P},
